@model ENTIDADES.REPORTES.REPORTE_DESPACHOS

@{
    ViewData["Title"] = "Reporte";
}

<h1>Edit</h1>

<h4>DESPACHO</h4>
<hr />
<div class="row">
    <div class="col-md-6">
        <form asp-action="Reporte">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <a class="btn btn-outline-primary" asp-action="Create"> <i class="fas fa-arrow-alt-circle-left"></i> Regresar</a>
            <input type="submit" value="Generar Reporte" class="btn btn-primary">
            
            <div class="form-group">
                <label asp-for="ID_BOMBA" class="control-label"></label>
                <select asp-for="ID_BOMBA" class="form-control" asp-items="ViewBag.ID_BOMBA"></select>
                <span asp-validation-for="ID_BOMBA" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="FECHA_DESPACHO" class="control-label"></label>
                <input asp-for="FECHA_DESPACHO" class="form-control" />
                <span asp-validation-for="FECHA_DESPACHO" class="text-danger"></span>
            </div>

        </form>
    </div>
</div>

<div>
  
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
